{
  "manifest_version": 3,
  "name": "Replace Images",
  "version": "1.0",
  "description": "Replace all images on a page with test.jpg",
  "permissions": ["activeTab", "storage", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["test.jpg"],
      "matches": ["<all_urls>"]
    }
  ]
}
